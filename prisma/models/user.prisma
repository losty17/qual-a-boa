model User {
  id            Int            @id @default(autoincrement())
  createdAt     DateTime       @default(now())
  email         String         @unique
  name          String?
  profileImage  String?
  events        UserEvents[]
  teams         TeamMember[]
  notifications Notification[]
}

model Notification {
  id        Int      @id @default(autoincrement())
  userId    Int
  user      User     @relation(fields: [userId], references: [id])
  message   String
  isRead    Boolean  @default(false)
  createdAt DateTime @default(now())
}
